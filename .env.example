# MapLibre GL JS Configuration
# NO API KEY REQUIRED! MapLibre + OpenStreetMap is completely free!

# TMDb API Configuration
# Get your API key from: https://www.themoviedb.org/settings/api
# Both values can be found in your TMDb account settings
TMDB_API_KEY=your_tmdb_api_key_here
TMDB_API_READ_ACCESS_TOKEN=your_tmdb_read_access_token_here

# App Configuration
VITE_APP_TITLE=filmingmap - Discover Movie Filming Locations
VITE_PUBLIC_URL=http://localhost:3000

# Optional: Default map center and zoom
VITE_DEFAULT_LAT=43.0
VITE_DEFAULT_LNG=0.35
VITE_DEFAULT_ZOOM=2.88

# Geocoding Service (optional)
# Options: nominatim (default)
GEOCODING_SERVICE=nominatim

# Rate Limiting (optional)
# Delay in milliseconds between requests
GEOCODING_DELAY_MS=1100
TMDB_DELAY_MS=250
