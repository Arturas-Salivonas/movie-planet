{
  "name": "cinemap",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Interactive 3D/2.5D world map showing where films and TV shows were filmed",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "generate:geojson": "tsx scripts/generateGeoJSON.ts",
    "generate:index": "tsx scripts/generateSearchIndex.ts",
    "fetch:movies": "tsx scripts/fetch_movies.ts",
    "ingest:imdb": "tsx scripts/ingest_imdb.ts",
    "geocode:fix": "tsx scripts/geocode_fix.ts",
    "transform:geojson": "tsx scripts/transform_to_geojson.ts",
    "verify:locations": "tsx scripts/verifyLocations.ts",
    "generate:verification": "tsx scripts/generateVerificationGuide.ts",
    "scrape:imdb": "tsx scripts/scrapeIMDbLocations.ts",
    "test:scraper": "tsx scripts/testIMDbScraper.ts",
    "fetch:auto": "tsx scripts/fetchMoviesAuto.ts",
    "fetch": "tsx scripts/fetchAndTransform.ts",
    "clean:duplicates": "tsx scripts/cleanDuplicateLocations.ts"
  },
  "dependencies": {
    "@types/cheerio": "^0.22.35",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "fuse.js": "^7.0.0",
    "maplibre-gl": "^5.11.0",
    "puppeteer": "^24.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "axios": "^1.13.2",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.6"
  }
}
